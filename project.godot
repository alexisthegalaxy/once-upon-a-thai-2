}

[application]

config/name="Once upon a Thai"
config/description="Save Thailand from evil forces using Thai, a magical language whose words grant you power over reality."
run/main_scene="res://Menu/Main Menu.tscn"

[autoload]

Game="*res://Game.tscn"
Events="*res://Events/Events.gd"
SoundPlayer="*res://UI/SoundPlayer.tscn"
DistractorsHelper="*res://Test/DistractorsHelper.gd"
Quests="*res://Quests/quests.gd"
Save="*res://Save/save.gd"
Money="*res://Money/Money.gd"

[display]

window/size/width=320
window/size/height=180
window/size/test_width=1080
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

